{
  "version": 2,
  "public": true,
  "build": {
    "command": "npm install",
    "src": "api",
    "use": "@vercel/node"
  },
  "routes": [
    {
      "src": "/",
      "status": 308,
      "dest": "/api"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/welcome.html"
    }
  ]
}
